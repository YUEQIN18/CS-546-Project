<body>
  <div class="userProfileTitle">
    <h1>Welcome!!! {{user.firstname}} {{user.lastname}}</h1>
    <h2>Your Public Profile</h2>
    
  </div>
  <div class = "userProfile">
    <hr>
    <p> </p>
    <p> Firstname </p>
    <input type="text" id="userfirstname" value="{{user.firstname}}" size="30"/>
    <p> Lastname </p>
    <input type="text" id="userlastname" value="{{user.lastname}}" size="30"/>
    <p> Email Address </p>
    <input type="text" id="useremail" value="{{user.email}}" size="30"/>
    <p> Gender </p>
    <input type="text" id="usergender" value="{{user.gender}}" size="30"/>
    <p> City </p>
    <input type="text" id="usercity" value="{{user.city}}" size="30"/>
    <p> State </p>
    <input type="text" id="userstate" value="{{user.state}}" size="30"/>
    <p> Age </p>
    <input type="text" id="userage" value="{{user.age}}" size="30"/>
    <p> Description </p>
    <input type="text" id="userdescription" value="{{user.description}}" size="30"/>
    <p> Your Apointments </p>
    <input type="text" id="userappointment" value="{{user.appointments}}" size="30"/>
    <div class="userProfileButton">
      <button type="submit" class="button"><span>submit</span> </button>
    </div>
  </div>
  
</body>